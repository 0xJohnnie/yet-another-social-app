{
  "name": "yet-another-social-app",
  "packageManager": "yarn@3.6.4",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "static": "next build && next export",
    "typecheck": "tsc --noEmit",
    "f": "next lint && prettier --write \"**/*.{ts,tsx}\"",
    "d": "yarn f && next dev",
    "b": "yarn f && next build",
    "s": "yarn b && next start",
    "u": "yarn yarn-upgrade-all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0xJohnnie/yet-another-social-app"
  },
  "author": "0xJohnnie",
  "dependencies": {
    "@ducanh2912/next-pwa": "^9.7.2",
    "@mantine/core": "^7.1.3",
    "@mantine/hooks": "^7.1.3",
    "@safe-global/auth-kit": "^1.2.1",
    "@safe-global/safe-react-components": "^2.0.6",
    "@tabler/icons-react": "^2.39.0",
    "@vercel/analytics": "^1.1.1",
    "@web3auth/base": "4.6.0",
    "@web3auth/modal": "4.6.2",
    "@web3auth/openlogin-adapter": "4.6.0",
    "alchemy-sdk": "^2.10.1",
    "cookies-next": "^4.0.0",
    "dayjs": "^1.11.10",
    "next": "^13.5.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "use-pull-to-refresh": "^2.2.1",
    "viem": "^1.16.6",
    "wagmi": "^1.4.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.5.5",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^13.5.5",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.31",
    "postcss-preset-mantine": "^1.9.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "yarn-upgrade-all": "^0.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": "please-use-yarn",
    "yarn": ">= 3.6.3"
  },
  "yarn-upgrade-all": {
    "ignore": [
      "react",
      "react-dom",
      "@safe-global/auth-kit",
      "@web3auth/base",
      "@web3auth/modal",
      "@web3auth/openlogin-adapter"
    ]
  }
}
